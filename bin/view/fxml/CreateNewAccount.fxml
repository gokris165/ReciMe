<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateNewAccountController">
   <children>
      <Button layoutX="68.0" layoutY="144.0" mnemonicParsing="false" onAction="#createButton" text="Create" />
      <Button layoutX="168.0" layoutY="144.0" mnemonicParsing="false" onAction="#cancelButton" text="Cancel" />
      <TextField fx:id="userID" layoutX="57.0" layoutY="25.0" promptText="Username" />
      <TextField fx:id="passID" layoutX="57.0" layoutY="69.0" promptText="Password" />
      <Label fx:id="errorLabel" layoutX="44.0" layoutY="106.0" opacity="0.0" prefHeight="31.0" prefWidth="213.0" text="Username/Password too short" textFill="RED" />
   </children>
</AnchorPane>
